<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="solis.freddy.CarRentalSystem"
    android:versionCode="1"
    android:versionName="1.0"
     >

    <uses-sdk android:minSdkVersion="8" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name" >
        <activity
            android:name=".CarRentalSystemActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CreateAccount"
            android:label="@string/createAccount" >
        <intent-filter>
            <action android:name="solis.freddy.CarRentalSystem.CreateAccount"/>
            <category android:name="android.intent.category.DEFAULT"/>
        </intent-filter>
       </activity> 
		<activity
            android:name=".SelectDate"
            android:label="@string/selectdate" >
        <intent-filter>
            <action android:name="solis.freddy.CarRentalSystem.SelectDate"/>
            <category android:name="android.intent.category.DEFAULT"/>
        </intent-filter>
       </activity>   
       <activity
            android:name=".SelectCar"
            android:label="@string/selectcar" >
        <intent-filter>
            <action android:name="solis.freddy.CarRentalSystem.SelectCar"/>
            <category android:name="android.intent.category.DEFAULT"/>
        </intent-filter>
       </activity> 
       <activity
            android:name=".LogInSystem"
            android:label="@string/loginsystem" >
        <intent-filter>
            <action android:name="solis.freddy.CarRentalSystem.LogInSystem"/>
            <category android:name="android.intent.category.DEFAULT"/>
        </intent-filter>
       </activity> 
        <activity
            android:name=".CancelReservation"
            android:label="@string/cancelreservation" >
        <intent-filter>
            <action android:name="solis.freddy.CarRentalSystem.CancelReservation"/>
            <category android:name="android.intent.category.DEFAULT"/>
        </intent-filter>
       </activity> 
        <activity
            android:name=".RemoveReservation"
            android:label="@string/cancelreservation" >
        <intent-filter>
            <action android:name="solis.freddy.CarRentalSystem.RemoveReservation"/>
            <category android:name="android.intent.category.DEFAULT"/>
        </intent-filter>
       </activity> 
        <activity
            android:name=".AminWindow"
            android:label="@string/tranhistory" >
        <intent-filter>
            <action android:name="solis.freddy.CarRentalSystem.AminWindow"/>
            <category android:name="android.intent.category.DEFAULT"/>
        </intent-filter>
       </activity> 
       
           
    </application>

</manifest>